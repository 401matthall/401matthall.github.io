---
permalink: /tags/
layout: layouts/base.njk
---
<div class="p-3 p-md-5">
<h2>hashTags</h2>
<div class="p-3">
<ul>
{% for tag in collections.tagList | filterTagList %}
  {% set tagUrl %}/tags/{{ tag | slug }}/{% endset %}
<li>  <a href="{{ tagUrl | url }}" class="text-white">#{{ tag }}</a></li>
{% endfor %}
</ul>
</div>
</div>